{
    "Logging": {
        "IncludeScopes": false,
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information",
            "SetMinimumLevel":  "Debug"
        }
    },

    "VaultOptions": {
        "Server": "http://localhost:8300",
        // "RoleId": "",
        // "SecretId": "",
        "TokenId": "root_dev_token",
        "Prefix": "secret",
        "Secrets": [
            "connectionString",
            "option1"
        ]
    },
    "VaultSeeder": [
        {
            "key": "secret/option1",
            "values": [
                "option1",
                "value"
            ]
        },
        {
            "key": "secret/connectionString",
            "values": [
                "connectionString",
                "some connection"
            ]
        }
    ]
}